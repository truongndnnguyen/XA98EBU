<!--main category-->
<li role="separator" class="watchzone-folder watchzone-folder-closed" data-category="{{name}}">
    <input id="{{name}}" category1="{{nameClean name}}" feedType="{{filterType}}" type="checkbox" class="emv-checkbox">
    <label for="{{name}}">{{name}} </label>
    <!--<span class="filter-folder-open-control fa fa-caret-right"></span>-->
    <span class="watchzone-folder-close-control pull-right">
        <img class="hidden-close icon-media-arrow_down" alt="Arrow down icon to indicate collapsed header" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" />
        <img class="visible-close icon-media-arrow_up" alt="Arrow up icon to indicate expanded header" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" />
    </span>
</li>